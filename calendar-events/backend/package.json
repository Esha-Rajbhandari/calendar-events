    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "nodemon": "^3.1.0",
    "npx": "^10.2.2",
    "pg": "^8.11.5",
    "query-string": "^9.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
