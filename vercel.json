{
  "version": 2,
  "outputDirectory": "dist",
  "builds": [{ "src": "packages/*", "use": "@vercel/node" }],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/calendar-events/backend/app/index.ts"
    },
    { "source": "/(.*)", "destination": "/calendar-events/web-app/$1" }
  ]
}
